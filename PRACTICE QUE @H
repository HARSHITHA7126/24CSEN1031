//AREA OF CIRCLE
INPUT======================================
#include<stdio.h>
int main (){
  float radius;
   printf("ENTER RADIUS:");
   scanf("%f",&radius);
   
   printf("AREA IS :%f", 3.14*radius*radius);
       return 0;
}
