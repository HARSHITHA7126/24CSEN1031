    float b=20.3;
    float  c = a + b;
    printf("Sum of the two numbers is %d" , c);
    return 0;
}





// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int a,b ;
    printf("Enter a value for a:");
    scanf("%d", &a);
    printf("Enter a value for b:");
    scanf("%d", &b);
    int c = a + b;
    scanf("Sum of the two numbers is %d" , c);
    return 0;
}



