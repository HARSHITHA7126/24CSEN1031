// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int a,b ;
    printf("Enter a value for a:");
    scanf("%d", &a);
    printf("Enter a value for b:");
    scanf("%d", &b);
    int c = a + b;
    scanf("Sum of the two numbers is %d" , c);
    return 0;
}




**CODE RAN SUCCESSFULLY** 


// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a;
    printf("Enter an integer (positive or negative):");
    scanf("%d",&a);
    if(a>0) {
        printf("The number %d is postive",a);
        if (a%2 == 0);
        printf("\n The number is even");
        
    }

    return 0;
}










