#include <stdio.h>

int main() {
    int num,i,flag=0;
    
    // Write C code here
    printf("Enter a number:");
    scanf("%d",&num);
    for (i=2;i<=num/2;++i){
    if (num%i==0){
      flag=1;
      break;
        }
    }
    if (flag==0)
    printf("%d is a prime number.\n",num);
    else
    printf("%d is not a prime number.\n",num);
                     return 0;
}



#include <stdio.h>

#include <stdio.h>

int main() {
    int a;
    printf("Enter an integer (positive or negative): ");
    scanf("%d", &a);
    if (a > 0) {
      printf("The nubmber %d is positive", a);
      if (a %2 == 0)
        printf("\nThe number is even");
      else
        printf("\nThe number is odd");
    }
    else if (a < 0)
      printf("The number %d is negative", a);
    else
        printf("The number %d is zero", a);
    
    return 0;
}



#include <stdio.h>

int main() {
    int a;
    printf("Enter an integer (positive or negative):");
    scanf("%d",&a);
    if(a>0) {
        printf("The number %d is postive",a);
        if (a%2 == 0);
        printf("\n The number is even");
        else 
        printf("\nThe number is odd");
        
        }
    
    else if (a<0)
    printf("The number %d is negative", a);
    else
    printf("The number %d is zero", a);
    return 0;
}


