// Program to find area of circle
#include <stdio.h>

int main() {
   printf("Enter the radius of the circle: "); 
   float radius ;
   scanf("%f" , &radius);
   printf("Area of the circle is %f",3.14 * radius* radius);
 
       return 0;
}





