#include <stdio.h>

int main() {
   printf("Enter the radius of the circle: "); 
  int radius ;
   scanf("%d" , &radius);
   printf("Area of the circle is %f",3.14 * radius* radius);
 
       return 0;
}











#include <stdio.h>

int main() {
    int num,i,flag=0;
    
    // Write C code here
    printf("Enter a number:");
    scanf("%d",&num);
    for (i=2;i<=num/2;++i){
    if (num%i==0){
      flag=1;
      break;
        }
    }
    if (flag==0)
    printf("%d is a prime number.\n",num);
    else
    printf("%d is not a prime number.\n",num);
                     return 0;
}
















